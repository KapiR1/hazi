@using Olimpikonok.Models;
@{
    ViewData["Title"] = "Országok Adatai";
}

<div class="text-center">
    <h1 class="display-4">Országok Adatai</h1>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Név</th>
                <th>Főváros</th>
                <th>Népesség</th>
            </tr>
        </thead>
            <tbody>
            @foreach (Orszag orszag in Model)
            {
                <tr>
                    <td>@orszag.Nev</td>
                    <td>@orszag.Fovaros</td>
                    <td>@orszag.Nepesseg</td>
                </tr>
            }
            </tbody>
        </table>
</div>
